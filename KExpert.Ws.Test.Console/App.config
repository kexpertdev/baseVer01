<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="soapEndpoint">
                    <security mode="TransportWithMessageCredential" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://localhost/KExpert.Ws/Service.svc"
                binding="basicHttpBinding" bindingConfiguration="soapEndpoint"
                contract="ServiceReference.IService" name="soapEndpoint" />
        </client>
    </system.serviceModel>
</configuration>